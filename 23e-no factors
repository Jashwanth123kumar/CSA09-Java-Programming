import java.util.*;
public class nooffactors {
public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		System.out.print("Enter the number : ");
		int n=s.nextInt();
		int fact=0;
		for(int i=1;i<=n;i++)
		{
		 if(n%i==0)
		 fact=fact+1;
		}
		System.out.print("Number of factors = "+fact);
		System.out.print("\nThe factors of "+n+"is ");
		for(int i=1;i<=n;i++)
		{
		 if(n%i==0)
			 System.out.print(i+" ");
		}
}
}
