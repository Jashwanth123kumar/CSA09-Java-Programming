import java.util.Scanner;
class Strings
{
	public static void main(String[] args)
	{
		Scanner s=new Scanner(System.in);
		System.out.print("enter the number");
		int a = s.nextInt();
		int n = a;
		int arm=0;
		while(n != 0)
		{
			int rem= n % 10;
			arm= arm +(rem * rem * rem);
			n= n / 10;
		}
		if (a == arm) {
            System.out.print("Armstrong");
        } else {
            System.out.print("Not Armstrong");
        }
		
	}
}
