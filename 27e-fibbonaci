package java2;
import java.util.*;
public class fibnoci {
	public static void main(String[] args) {
		Scanner input=new Scanner(System.in); 
		System.out.print("Enter the number : ");
		int n=input.nextInt(); 
		int n1=0,n2=1; 
		for(int i=0;i<n;i++) 
		{ 
		System.out.print(n1+" "); 
		int n3=n1+n2; 
		n1=n2; 
		n2=n3; 
		} 

	}

}
