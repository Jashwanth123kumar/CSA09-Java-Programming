package java1;
import java.util.*;
public class pattrenofsqare {
public static void main (String[] args) {
	int i,j,n,k=1;
	Scanner s=new Scanner(System.in);
	System.out.println("Enter the number :");
	n=s.nextInt();
	for (i=1;i<=n;i++)
	{ for (j=1;j<=i;j++)
	  { System.out.print(k*k+" ");
	  k++; }
	 System.out.println();
}
}
}
