import java.util.Scanner;
public class rev
{
    public static void main(String[] args)
    {
        Scanner s=new Scanner(System.in);
        System.out.print("enter the numbers");
        int a=s.nextInt();
        int b=0;
        while(a!=0)
        {
            int c=a%10;
            b=b*10+c;
            a=a/10;

        }
        System.out.println(b+"");
        s.close();

    }
}
