import java.util.Scanner;
public class doc
{
    public static void main(String[] args)
    {
        Scanner s=new Scanner(System.in);
        System.out.print("enter the number");
        int a=s.nextInt();
        System.out.print("enter the number");
        int b=s.nextInt();
        for(int i=1;i<=b;i++)
        {
         System.out.println(a+"x"+i+"="+(a*i));
        }
        
        
    }
}
