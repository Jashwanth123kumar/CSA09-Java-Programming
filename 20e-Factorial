package java2;
import java.util.*;
public class factorial {
	public static void main(String[] args) {
		Scanner input=new Scanner(System.in); 
		System.out.print("Enter the number : "); 
		int n=input.nextInt(); 
		int fact=1; 
		System.out.print("Factorial of "+n+" is "); 
		for(int i=1;i<=n;i++) 
		{ 
		fact=fact*i; 
		} 
		System.out.print(fact); 
		} 
	}
