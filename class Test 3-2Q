import java.util.Scanner;
public class rev
{
    public static void main(String[] args)
    {
        Scanner s=new Scanner(System.in);
        int a1=0; int a2=1;
        System.out.print("enter the number");
        int a=s.nextInt();
        for(int i=0;i<a;i++)
        {
            int a3=a1+a2;
            a1=a2;
            a2=a3;
            System.out.print(a1+" ");
            s.close();
        }

     
    }
}
